<Window x:Class="MatchRedux.FetchIon"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="FetchIon" Height="148" Width="639">
    <Grid Height="371" Width="466">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <StackPanel Orientation="Horizontal" Grid.Row="0">
            <Button Content="Start" Click="Start_Click" IsEnabled="{Binding IsStartEnabled}"/>
            <Button Content="Cancel" Click="Cancel_Click" IsEnabled="{Binding IsCancelEnabled}"/>
            <CheckBox Content="Contributors" IsChecked="{Binding IsIonContributors}"/>
            <CheckBox Content="Tags" IsChecked="{Binding IsTags}"/>
            <CheckBox Content="Categories" IsChecked="{Binding IsCategories}"/>
        </StackPanel>
    </Grid>
</Window>
